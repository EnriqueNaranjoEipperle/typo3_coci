  <file source-language="en" datatype="plaintext" original="EXT:recycler/Resources/Private/Language/locallang_tasks.xlf" date="2011-10-17T20:22:35Z" product-name="recycler" target-language="es">
    <header/>
    <body>
      <trans-unit id="cleanerTaskTitle" resname="cleanerTaskTitle" approved="yes">
        <source>Remove deleted records</source>
        <target state="final">Borrar registros eliminados</target>
      </trans-unit>
      <trans-unit id="cleanerTaskDescription" resname="cleanerTaskDescription" approved="yes">
        <source>Deletes rows from the database which are flagged "deleted"</source>
        <target state="final">Borrar filas de la base de datos que estén con la etiqueta de "eliminado"</target>
      </trans-unit>
      <trans-unit id="cleanerTaskPeriod" resname="cleanerTaskPeriod" approved="yes">
        <source>Delete entries older than (in days)</source>
        <target state="final">Borrar entradas de mas de X días</target>
      </trans-unit>
      <trans-unit id="cleanerTaskTCA" resname="cleanerTaskTCA" approved="yes">
        <source>Tables</source>
        <target state="final">Tablas</target>
      </trans-unit>
      <trans-unit id="cleanerTaskDescriptionTables" resname="cleanerTaskDescriptionTables" approved="yes">
        <source>Tables: %s</source>
        <target state="final">Tablas: %s</target>
      </trans-unit>
      <trans-unit id="cleanerTaskDescriptionDays" resname="cleanerTaskDescriptionDays" approved="yes">
        <source>Older than %d day(s)</source>
        <target state="final">Más antiguo que %d día(s)</target>
      </trans-unit>
      <trans-unit id="cleanerTaskErrorPeriod" resname="cleanerTaskErrorPeriod" approved="yes">
        <source>The period has to be an integer and greater than zero.</source>
        <target state="final">El periodo debe ser un número entero mayor que cero.</target>
      </trans-unit>
      <trans-unit id="cleanerTaskErrorTCAempty" resname="cleanerTaskErrorTCAempty" approved="yes">
        <source>You have to select at least one table.</source>
        <target state="final">Debe seleccionar al menos una tabla</target>
      </trans-unit>
      <trans-unit id="cleanerTaskErrorTCANotSet" resname="cleanerTaskErrorTCANotSet" approved="yes">
        <source>The table "%s" is not in the TCA.</source>
        <target state="final">La tabla "%s" no está en el TCA.</target>
      </trans-unit>
    </body>
